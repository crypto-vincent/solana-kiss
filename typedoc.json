{
  "entryPoints": ["src/**/*.ts"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "name": "solana-kiss",
  "readme": "none",
  "includeVersion": true,
  "outputFileStrategy": "modules",
  "useCodeBlocks": true,
  "sanitizeComments": true,
  "navigationJson": "docs/.vitepress/typedoc-navigation.json"
}
